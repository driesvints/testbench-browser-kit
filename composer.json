{
    "name": "orchestra/testbench-browser-kit",
    "description": "Laravel Browser Kit Testing Helper for Packages Development",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Testbench\\BrowserKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Testbench\\BrowserKit\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "laravel/browser-kit-testing": "~2.0",
        "orchestra/testbench": "~3.6.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "orchestra/database": "~3.6.0",
        "phpunit/phpunit": "~6.0"
    },
    "suggest": {
        "orchestra/database": "Allow to use --realpath migration for testing (~3.6)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.6-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
